{
    "buildCommand": "npx prisma generate && npm run build",
    "outputDirectory": "build",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ]
}